{
    "extends": [
        "wesbos",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": false,
                "singleQuote": true,
                "bracketSpacing": true
            }
        ],
        "class-methods-use-this": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "strict": 0,
        "semi": 0,
        "no-unused-vars": 0
    }
}